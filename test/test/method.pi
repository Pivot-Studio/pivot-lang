use core::panic;
pub struct impl_struct {
    x: i64;
}
/// ANCHOR: impl
impl impl_struct {
    pub fn add1() void {
        self.x = self.x + 1;
        return;
    }

    pub fn set(x: i64) void {
        self.x = x;
        return;
    }

}

/// ANCHOR_END: impl

pub fn test_impl() void {
    let x = impl_struct{
        x: 99
    };
    x.set(114514);
    panic::assert(x.x == 114514);
    x.add1();
    panic::assert(x.x == 114515);
    return;
}

