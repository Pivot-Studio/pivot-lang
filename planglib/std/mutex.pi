pub struct Mutex{}

pub fn create_mutex(mutex: **Mutex) u64;

fn lock_mutex(mutex: *Mutex) u64;


pub fn lock_mutexWrap(mutex: *Mutex) u64{
    let res:u64;
    gc::DioGC__stuck_begin();
    res = lock_mutex(mutex);
    gc::DioGC__stuck_end();
    return res;
}

pub fn unlock_mutex(mutex: *Mutex) u64;

pub fn drop_mutex(mutex: *Mutex) u64;